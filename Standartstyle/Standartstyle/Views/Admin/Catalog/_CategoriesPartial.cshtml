@model IEnumerable<Standartstyle.Models.GoodsCategoryModel>

<div id="categories">
    <div class="col-md-3">
        <span class="panel-title">Категории</span>
    </div>
    <div class="col-md-9">
        @foreach (var category in Model)
        {
            <div class="col-lg-3 col-md-4 col-xs-6">
                <div class="category">
                    <a>
                        <span>@category.Name</span>
                    </a>
                </div>
            </div>
        }
    </div>
</div>