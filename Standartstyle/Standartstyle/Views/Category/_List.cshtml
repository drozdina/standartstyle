@model IEnumerable<Standartstyle.Models.GoodsCategoryModel>

<div class="container">

    <div class="row">

        <div class="col-lg-12">
            <h1 class="page-header">Thumbnail Gallery</h1>
        </div>

        @foreach (var good in Model)
        {
            <div class="col-lg-3 col-md-4 col-xs-6 thumb">
                <a class="thumbnail" href="#">@good.Name</a>
            </div>
        }
    </div>
</div>
